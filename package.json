{
  "name": "hbot",
  "version": "0.0.1",
  "description": "Discord bot",
  "main": "src/index.js",
  "engines": {
    "node": ">=16.13.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node -r dotenv/config build/index.js",
    "dev": "concurrently \"tsc --watch\" \"nodemon -r dotenv/config build/index.js\" ",
    "ts-build-auto": "tsc src/index.tsx --watch",
    "help": "node --loader ts-node/esm src/helps.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HBooot/discord-bot.git"
  },
  "keywords": [
    "discord",
    "bot"
  ],
  "author": "Nathan-html",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HBooot/discord-bot/issues"
  },
  "homepage": "https://github.com/HBooot/discord-bot#readme",
  "dependencies": {
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "firebase": "^9.7.0",
    "mongoose": "^6.3.2",
    "node-cron": "^3.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "concurrently": "^7.1.0",
    "eslint": "^8.14.0",
    "nodemon": "^2.0.15",
    "typescript": "^4.6.4"
  }
}
